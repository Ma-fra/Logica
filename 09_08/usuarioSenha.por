programa {
	funcao inicio() 
	{
		cadeia nome, senha
		escreva("Digite o nome do usuário: ")
		leia(nome)
		
		escreva("Digite a senha do usuário")
		leia(senha)
		
		se(nome=="Marcelle" ou nome=="marcelle") e senha=="123"){
		    escreva("Bem vindo ao Sistema!")
		}senao{
		    escreva("Acesso negado")
		}
		}
	}
}
